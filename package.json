        "@middy/s3-key-normalizer": "2.5.7",
        "@middy/warmup": "4.6.5",
        "@pulsifi/fn": "1.14.0",
        "@sentry/serverless": "7.74.1",
        "form-data": "4.0.0",
        "http-error-handler": "0.3.1",
