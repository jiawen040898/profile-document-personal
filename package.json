{
    "name": "profile-document-fn",
    "version": "1.13.0",
    "author": "Pulsifi <dev-team@pulsifi.me>",
    "license": "UNLICENSED",
    "description": "Profile Document Fn",
    "private": true,
    "scripts": {
        "prepare": "is-ci || husky install",
        "build": "yarn install --frozen-lockfile --prefer-offline && yarn install --frozen-lockfile --prefer-offline --cwd layer/nodejs",
        "deploy": "serverless deploy -v",
        "remove": "serverless remove -v",
        "log-upload": "serverless logs -t -f upload",
        "format": "pretty-quick --staged && prettier --loglevel \"warn\" --debug-check \"src/**/*.ts\" \"tests/**/*.ts\"",
        "lint": "yarn format && eslint \"{src,tests}/**/*.ts\" --fix",
        "test": "jest --watch",
        "test:ci": "jest --ci --coverage",
        "cdk": "cdk",
        "biome:format": "biome format --write ./cdk",
        "biome:lint": "biome lint ./cdk",
        "biome:check": "biome check --apply ./cdk"
    },
    "devDependencies": {
        "@actions/core": "1.10.1",
        "constructs": "10.3.0",
        "aws-cdk": "2.141.0",
